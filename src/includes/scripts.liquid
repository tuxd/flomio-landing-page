<script>
  ( function( $ ) {
    // Init Skrollr
    // var s = skrollr.init({
    //     render: function(data) {
    //         //Debugging - Log the current scroll position.
    //         //console.log(data.curTop);
    //     },
    //     forceHeight:true
    // });

  var $window = $(window);

  if ($window.width() > 767) {
    skrollr.init({
      forceHeight:true
    });
  }

  $window.on('resize', function () {
    if ($window.width() <= 767) {
      skrollr.init({
         forceHeight:true
      }).destroy();
    } else {
        skrollr.init({
          forceHeight:true
        });
    }
  });

  
  
  function resizeSlides(){

    var sectionHeight = $window.height();
    
    if(sectionHeight > 600) {
      $(".homeSlide, .bcg, .minHeight").height(sectionHeight);
      // $(".bcg").height(sectionHeight);

    } else {
      $(".homeSlide, .bcg, .minHeight").height(600);
      // $(".bcg").height(600);
    }

  }
  
  function centerSecondFrame(){
     var $slide2Content = $("#slide-2 .hsContent");
     var $contentHeight = $slide2Content.height();
     var $midPoint = ($window.height() - ($contentHeight/2))/2;
     $slide2Content.css({ paddingTop: $midPoint + "px" });
    };

    var menuOffset = $("#slide-2").offset().top + $("#buttonRow").offset().top;

    $window.scroll(function() {

      var windowOffset = $window.scrollTop();

      console.log(windowOffset);
      console.log(menuOffset);

      if((menuOffset - windowOffset) < 10) {
        $("#buttonRow-fixed").fadeIn();
        $("#buttonRow").css({ display:"none" });
      }
      else {
        $("#buttonRow-fixed").fadeOut();
        $("#buttonRow").css({ display:"block" });
      }
    });

    $window.resize(function(){
      resizeSlides();
      centerSecondFrame();
    });

    resizeSlides();
    centerSecondFrame();



} )( jQuery );
</script>
